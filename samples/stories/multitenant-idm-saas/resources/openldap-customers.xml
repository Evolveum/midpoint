					</expression>
				</outbound>
			</attribute>
			<attribute>
				<ref>ri:description</ref>
				<outbound>
					<source>
						<path>description</path>
					</source>
				</outbound>
			</attribute>
			<dependency>
				<kind>generic</kind>
				<intent>ou-customer</intent>
				<strictness>relaxed</strictness>
			</dependency>
			<configuredCapabilities>
				<cap:pagedSearch>
