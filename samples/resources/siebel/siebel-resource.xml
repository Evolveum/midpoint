								                </xsd:appinfo>
							              </xsd:annotation>
						            </xsd:element>
						            <xsd:element minOccurs="0" name="PreferredCommunication" type="xsd:string">
							              <xsd:annotation>
								                <xsd:appinfo>
									                  <a:displayOrder>300</a:displayOrder>
									                  <ra:frameworkAttributeName>PreferredCommunication</ra:frameworkAttributeName>
								                </xsd:appinfo>
							              </xsd:annotation>
						            </xsd:element>
						            <xsd:element minOccurs="0" name="JobTitle" type="xsd:string">
							              <xsd:annotation>
								                <xsd:appinfo>
									                  <a:displayOrder>310</a:displayOrder>
									                  <ra:frameworkAttributeName>JobTitle</ra:frameworkAttributeName>
								                </xsd:appinfo>
							              </xsd:annotation>
						            </xsd:element>
						            <xsd:element minOccurs="0" name="Id" type="xsd:string">
							              <xsd:annotation>
								                <xsd:appinfo>
									                  <a:displayOrder>100</a:displayOrder>
									                  <a:access>read</a:access>
									                  <ra:nativeAttributeName>Id</ra:nativeAttributeName>
									                  <ra:frameworkAttributeName>__UID__</ra:frameworkAttributeName>
								                </xsd:appinfo>
							              </xsd:annotation>
						            </xsd:element>
					          </xsd:sequence>
				        </xsd:complexType>
			      </xsd:schema>
      </definition>
   </schema>
   <schemaHandling>
      <objectType>
         <kind>account</kind>
         <default>true</default>
         <objectClass>ri:AccountObjectClass</objectClass>
         <attribute>
            <c:ref>ri:LoginName</c:ref>
            <outbound>
               <strength>strong</strength>
               <source>
                  <c:path>$focus/name</c:path>
               </source>
               <expression>
                  <script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                          xsi:type="c:ScriptExpressionEvaluatorType">
                     <code>
        								basic.uc(name?.getOrig());	
        							</code>
                  </script>
               </expression>
            </outbound>
         </attribute>                          
         <attribute>
            <c:ref>ri:LastName</c:ref>
            <outbound>
               <strength>strong</strength>
               <source>
                  <c:path>$focus/familyName</c:path>
               </source>
            </outbound>
         </attribute>
         <attribute>
            <c:ref>ri:EMailAddr</c:ref>
            <outbound>
               <strength>strong</strength>
               <source>
                  <c:path>$focus/emailAddress</c:path>
               </source>
            </outbound>
         </attribute>
         <attribute>
            <c:ref>ri:FirstName</c:ref>
            <outbound>
               <strength>strong</strength>
               <source>
                  <c:path>$focus/givenName</c:path>
               </source>
            </outbound>
         </attribute>
         
         <activation>
            <existence>
               <outbound>
                  <strength>weak</strength>
                  <expression>
                     <variable>
                        <name>name</name>
                        <c:path>$focus/name</c:path>
                     </variable>
                     <script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                             xsi:type="c:ScriptExpressionEvaluatorType">
                            <code>
                        			focusExists
                        		</code>
                     </script>
                  </expression>
               </outbound>
            </existence>
            <administrativeStatus>
               <outbound>
                  <strength>strong</strength>
                  <expression>
                     <script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                             xsi:type="c:ScriptExpressionEvaluatorType">
                        <code>
	                            import com.evolveum.midpoint.xml.ns._public.common.common_3.ActivationStatusType;
	                            	                            	                                                        
	                            ActivationStatusType.DISABLED;
							         </code>
                     </script>
                  </expression>
                  <condition>
                     <variable>
                        <name>identityEffectiveStatus</name>
                        <c:path>$focus/activation/effectiveStatus</c:path>
                     </variable>
                     <script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                             xsi:type="c:ScriptExpressionEvaluatorType">
                        <code>
                     			import com.evolveum.midpoint.xml.ns._public.common.common_3.ActivationStatusType;
                     		
                     			// disabled identity must always have all linked accounts DISABLED (strong)
                     			identityEffectiveStatus != ActivationStatusType.ENABLED;
                     		</code>
                     </script>
                  </condition>
               </outbound>
               <outbound>
                  <strength>normal</strength>
                  <expression>
                     <script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                             xsi:type="c:ScriptExpressionEvaluatorType">
                        <code>
	                            import com.evolveum.midpoint.xml.ns._public.common.common_3.ActivationStatusType;
	                            	                            	                            
	                            (legal)?input:ActivationStatusType.DISABLED; // disable instead of delete
							           </code>
                     </script>
                  </expression>
               </outbound>
            </administrativeStatus>
         </activation>
      </objectType>
   </schemaHandling>
   <capabilities>      
      <native xmlns:cap="http://midpoint.evolveum.com/xml/ns/public/resource/capabilities-3"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:type="c:CapabilityCollectionType">
         <cap:addRemoveAttributeValues/>
         <cap:activation>
            <cap:status/>
         </cap:activation>
         <cap:testConnection/>
         <cap:create/>
         <cap:read/>
         <cap:update/>
         <cap:script>
            <cap:host>
               <cap:type>connector</cap:type>
            </cap:host>
         </cap:script>
      </native>
      <configured xmlns:cap="http://midpoint.evolveum.com/xml/ns/public/resource/capabilities-3"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:type="c:CapabilityCollectionType">
         <cap:pagedSearch/>
         <cap:addRemoveAttributeValues>
            <cap:enabled>false</cap:enabled>
         </cap:addRemoveAttributeValues>
         <cap:testConnection>
            <cap:enabled>true</cap:enabled>
         </cap:testConnection>
         <cap:read>
            <cap:enabled>true</cap:enabled>
         </cap:read>
         <cap:script>
            <cap:enabled>false</cap:enabled>
         </cap:script>
         <cap:activation>
            <cap:status>
               <cap:enabled>true</cap:enabled>
            </cap:status>
         </cap:activation>
         <cap:create>
            <cap:enabled>true</cap:enabled>
         </cap:create>
         <cap:update>
            <cap:enabled>true</cap:enabled>
         </cap:update>
         <cap:delete>
            <cap:enabled>false</cap:enabled>
         </cap:delete>
      </configured>
   </capabilities>
   <projection>
      <assignmentPolicyEnforcement>relative</assignmentPolicyEnforcement>
      <legalize>false</legalize>
   </projection>
   <consistency>
      <connectorErrorCriticality>
         <network>false</network>
         <schema>false</schema>
      </connectorErrorCriticality>
   </consistency>
   <synchronization>
      <objectSynchronization>
         <name>Account sync</name>
         <objectClass>ri:AccountObjectClass</objectClass>
         <kind>account</kind>
         <intent>default</intent>
         <focusType>UserType</focusType>
         <enabled>true</enabled>
         <correlation>
            <q:equal>
               <q:matching>polyStringNorm</q:matching>
               <q:path>c:name</q:path>
               <expression>
                  <path>$shadow/attributes/LoginName</path>
               </expression>
            </q:equal>
         </correlation>
         <limitPropagation>true</limitPropagation>
         <reaction>
            <situation>linked</situation>
            <synchronize>false</synchronize>
         </reaction>
         <reaction>
            <situation>deleted</situation>
            <synchronize>true</synchronize>
            <action>
               <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/action-3#unlink</handlerUri>
            </action>
         </reaction>
         <reaction>
            <situation>unlinked</situation>
            <synchronize>true</synchronize>
            <action>
               <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/action-3#link</handlerUri>
            </action>
         </reaction>
         <reaction>
            <situation>unmatched</situation>
            <synchronize>true</synchronize>
         </reaction>
      </objectSynchronization>
   </synchronization>
</resource>