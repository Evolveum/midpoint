<task oid="12f07ab1-41c3-4dba-bf47-3d2a032fa555"
        xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
        xmlns:q="http://prism.evolveum.com/xml/ns/public/query-3"
        xmlns:ext="http://midpoint.evolveum.com/xml/ns/repo-common-test/extension">
    <name>task-bucketed-tree-analysis</name>
    <ownerRef oid="00000000-0000-0000-0000-000000000002"/>
    <executionState>runnable</executionState>
    <activity>
        <composition>
            <activity>
                <order>1</order>
                <identifier>first</identifier>
                <work>
                    <extension>
                        <ext:searchIterativeMock>
                            <ext:objectSet>
                                <type>SystemConfigurationType</type>
                            </ext:objectSet>
                            <ext:message>First (config): </ext:message>
                        </ext:searchIterativeMock>
                    </extension>
                </work>
                <executionMode>bucketAnalysis</executionMode>
                <reporting>
                    <reports>
                        <buckets/>
                        <items/>
                    </reports>
                </reporting>
            </activity>
            <activity>
                <order>2</order>
                <identifier>second</identifier>
                <work>
                    <extension>
                        <ext:searchIterativeMock>
                            <ext:objectSet>
                                <type>RoleType</type>
                                <query>
                                    <q:filter>
                                        <q:substring>
                                            <q:path>name</q:path>
                                            <q:value>r1</q:value>
                                            <q:anchorStart>true</q:anchorStart>
                                        </q:substring>
                                    </q:filter>
                                </query>
                            </ext:objectSet>
                            <ext:message>Third-A (role): </ext:message>
                        </ext:searchIterativeMock>
                    </extension>
                </work>
                <executionMode>bucketAnalysis</executionMode>
                <distribution>
                    <buckets>
                        <stringSegmentation>
                            <discriminator>name</discriminator>
                            <boundary>
                                <position>1</position>
                                <characters>r</characters>
                            </boundary>
                            <boundary>
                                <position>2</position>
                                <characters>1</characters>
                            </boundary>
                            <boundary>
                                <position>3</position>
                                <characters>0-9</characters>
                            </boundary>
                        </stringSegmentation>
                    </buckets>
                </distribution>
                <reporting>
                    <reports>
                        <buckets/>
                        <items/>
                    </reports>
                </reporting>
            </activity>
            <activity>
                <order>3</order>
                <identifier>third</identifier>
                <work>
                    <extension>
                        <ext:searchIterativeMock>
                            <ext:objectSet>
                                <type>RoleType</type>
                                <query>
                                    <q:filter>
                                        <q:substring>
                                            <q:path>name</q:path>
                                            <q:value>r</q:value>
                                            <q:anchorStart>true</q:anchorStart>
                                        </q:substring>
                                    </q:filter>
                                </query>
                            </ext:objectSet>
                            <ext:message>Fourth (role): </ext:message>
                        </ext:searchIterativeMock>
                    </extension>
                </work>
                <executionMode>bucketAnalysis</executionMode>
                <distribution>
                    <buckets>
                        <stringSegmentation>
                            <discriminator>name</discriminator>
                            <boundary>
                                <position>1</position>
                                <characters>r</characters>
                            </boundary>
                            <boundary>
                                <position>2</position>
                                <position>3</position>
                                <characters>0-9</characters>
                            </boundary>
                        </stringSegmentation>
                    </buckets>
                </distribution>
                <reporting>
                    <reports>
                        <buckets/>
                        <items/>
                    </reports>
                </reporting>
            </activity>
        </composition>
    </activity>
</task>
