<query xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-2a"
       xmlns="http://prism.evolveum.com/xml/ns/public/query-2">
    <and>
        <equal>
        <path>c:name</path>
            <matching>polyStringNorm</matching>
            <value>generic object</value>
        </equal>
        <equal>
            <path xmlns:p="http://example.com/p">c:extension/p:intType</path>
            <value>123</value>
        </equal>
    </and>
</query>
