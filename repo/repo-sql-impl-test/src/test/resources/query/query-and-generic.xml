<query xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
       xmlns="http://prism.evolveum.com/xml/ns/public/query-3">
   <filter>
    <and>
        <equal>
        <path>c:name</path>
            <matching>polyStringNorm</matching>
            <value>generic object</value>
        </equal>
        <equal>
            <path xmlns:p="http://example.com/p">c:extension/p:intType</path>
            <value>123</value>
        </equal>
    </and>
    </filter>
</query>
