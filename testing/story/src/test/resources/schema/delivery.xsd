<xsd:schema elementFormDefault="qualified"
            targetNamespace="http://midpoint.evolveum.com/xml/ns/story/delivery/ext"
            xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
            xmlns:a="http://prism.evolveum.com/xml/ns/public/annotation-3"
            xmlns:t="http://prism.evolveum.com/xml/ns/public/types-3"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:import namespace="http://prism.evolveum.com/xml/ns/public/types-3"/>
	<xsd:import namespace="http://midpoint.evolveum.com/xml/ns/public/common/common-3"/>

    <!-- role extension for rules -->

    <xsd:complexType name="RoleExtensionType">
        <xsd:annotation>
            <xsd:appinfo>
                <a:extension ref="c:RoleType"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="attribute1" type="t:ItemPathType" minOccurs="0">
            	<xsd:annotation>
            		<xsd:appinfo>
            			<a:indexed>false</a:indexed>
            		</xsd:appinfo>
            	</xsd:annotation>
            </xsd:element>
            <xsd:element name="attribute2" type="t:ItemPathType" minOccurs="0">
            	<xsd:annotation>
            		<xsd:appinfo>
            			<a:indexed>false</a:indexed>
            		</xsd:appinfo>
            	</xsd:annotation>
            </xsd:element>
            <xsd:element name="attribute3" type="t:ItemPathType" minOccurs="0">
            	<xsd:annotation>
            		<xsd:appinfo>
            			<a:indexed>false</a:indexed>
            		</xsd:appinfo>
            	</xsd:annotation>
            </xsd:element>
			<xsd:element name="value1" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:appinfo>
						<a:indexed>false</a:indexed>
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="value2" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:appinfo>
						<a:indexed>false</a:indexed>
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="value3" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:appinfo>
						<a:indexed>false</a:indexed>
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="remediatorRef" type="c:ObjectReferenceType" minOccurs="0">
				<xsd:annotation>
					<xsd:appinfo>
						<a:indexed>false</a:indexed>
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="roleRef" type="c:ObjectReferenceType" minOccurs="0">
				<xsd:annotation>
					<xsd:appinfo>
						<a:indexed>false</a:indexed>
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
        </xsd:sequence>
    </xsd:complexType>

	<!-- user extension -->

	<xsd:complexType name="UserExtensionType">
        <xsd:annotation>
            <xsd:appinfo>
                <a:extension ref="c:UserType"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="tpu" type="xsd:string" minOccurs="0">
            	<xsd:annotation>
            		<xsd:appinfo>
            			<a:indexed>true</a:indexed>
            		</xsd:appinfo>
            	</xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

</xsd:schema>
