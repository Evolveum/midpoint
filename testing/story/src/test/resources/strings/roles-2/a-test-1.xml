		</c:targetRef>
	</c:assignment>
	<c:assignment id="3">
		<c:targetRef type="c:RoleType">
			<filter>
				<q:equal>
					<q:path>name</q:path>
					<q:value>metarole-approval-role-approvers-all</q:value>
				</q:equal>
			</filter>
		</c:targetRef>
