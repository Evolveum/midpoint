<!-- Object modification description that is changing account of "guybrush" -->

<objectDelta
	xmlns:c='http://midpoint.evolveum.com/xml/ns/public/common/common-3'
	xmlns="http://prism.evolveum.com/xml/ns/public/types-3"
	xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance-3">
	<changeType>modify</changeType>
	<objectType>c:ShadowType</objectType>
	<oid>will be replaced by the test code</oid>
	<itemDelta>
		<modificationType>replace</modificationType>
		<path>c:attributes/ri:roomNumber</path>
		<value>cabin</value>
	</itemDelta>
	<itemDelta>
		<modificationType>add</modificationType>
		<path>c:attributes/ri:businessCategory</path>
		<value>capsize</value>
		<value>fighting</value>
	</itemDelta>
</objectDelta>
