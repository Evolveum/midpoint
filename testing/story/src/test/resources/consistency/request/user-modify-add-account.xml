<!-- Object modification description that is changing user "jack" by adding 
	a new account -->

<objectModification
	xmlns='http://midpoint.evolveum.com/xml/ns/public/common/api-types-3'
	xmlns:c='http://midpoint.evolveum.com/xml/ns/public/common/common-3'
	xmlns:t="http://prism.evolveum.com/xml/ns/public/types-3"
	xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance-3">
	<oid>c0c010c0-d34d-b33f-f00d-111111111111</oid>
	<modification>
		<t:modificationType>add</t:modificationType>
		<t:value>
			<c:assignment>
				<c:construction xmlns:icfs="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/resource-schema-3">
					<c:resourceRef oid="ef2bc95b-76e0-59e2-86d6-3d4f02d3ffff" />
					<c:objectClass>ri:inetOrgPerson</c:objectClass>
					<c:attributes>
						<ri:dn>uid=jackie,ou=people,dc=example,dc=com</ri:dn>
					</c:attributes>
				</c:construction>
			</c:assignment>
		</t:value>
	</modification>
</objectModification>
