Parameters:
 - name
 - oid
 - inducedOidList
-->
<role oid="$oid"
        xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3">
    <name>$name</name>

#foreach($inducedOid in $inducedOidList)
    <inducement>
        <targetRef oid="$inducedOid" type="RoleType"/>
    </inducement>
#end
</role>
