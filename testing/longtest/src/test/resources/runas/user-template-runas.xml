    <name>User Template: RunAs</name>

    <mapping>
        <name>org001</name>
        <authoritative>true</authoritative>
        <source>
            <path>organization</path>
        </source>
        <expression>
            <runAsRef oid="20b4d7c0-c9e9-11e7-887c-7fe1dc65a3ed"/> <!-- Robot -->
            <script>
                <code>'001' + organization + ": " + midpoint.getPrincipal().getUsername()</code>
            </script>
        </expression>
        <target>
            <path>organizationalUnit</path>
        </target>
    </mapping>

