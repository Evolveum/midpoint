<?xml version="1.0" encoding="UTF-8"?>
<weblogic-application xmlns="http://xmlns.oracle.com/weblogic/weblogic-application”
xmlns:xsi=”http://www.w3.org/2001/XMLSchema-instance
xsi:schemaLocation=”http://xmlns.oracle.com/weblogic/weblogic-application
http://xmlns.oracle.com/weblogic/weblogic-application/1.0/weblogic-application.xsd">
     <xml>
        <parser-factory>
            <saxparser-factory>com.sun.org.apache.xerces.internal.jaxp.SAXParserFactoryImpl</saxparser-factory>
            <document-builder-factory>com.sun.org.apache.xerces.internal.jaxp.DocumentBuilderFactoryImpl</document-builder-factory>
            <transformer-factory>net.sf.saxon.TransformerFactoryImpl</transformer-factory>
        </parser-factory>
    </xml>
    <application-param>
        <param-name>webapp.encoding.default</param-name>
        <param-value>UTF-8</param-value>
    </application-param>
    <prefer-application-packages>
    	<package-name>org.antlr.*</package-name>
        <!-- // for logging  -->
        <package-name>org.slf4j.*</package-name>
        <package-name>ch.qos.logback.*</package-name>
        <package-name>org.apache.commons.logging.*</package-name>
        <!-- // for jaxb  -->
        <!--package-name>com.sun.xml.*</package-name-->
        <!-- // for apache commons lang/io  -->
        <package-name>org.apache.commons.*</package-name>
		<!-- // for spring/hibernate -->
        <package-name>antlr.*</package-name>
        <package-name>org.springframework.*</package-name>
		<!-- // xml processing -->
        <!--package-name>javax.xml.*</package-name-->
        <!--package-name>javax.xml.stream.*</package-name-->
        <!--package-name>org.xml.sax.*</package-name-->
        <!--package-name>org.w3c.*</package-name-->
        <package-name>org.apache.xmlcommons.*</package-name>
        <package-name>net.sf.saxon.*</package-name>
        <package-name>com.ctc.wstx.*</package-name>
        <package-name>org.codehaus.groovy.*</package-name>
        <package-name>org.codehaus.stax2.*</package-name>
        <package-name>org.codehaus.staxmate.*</package-name>       
        <package-name>org.mozilla.*</package-name>
		<package-name>org.joda.*</package-name>
		<package-name>javax.persistence.*</package-name>
		<package-name>org.python.*</package-name>
		<package-name>org.apache.cxf.*</package-name>
		<!-- wls-cat -->
		<package-name>com.google.common.*</package-name>
		<!--
		<package-name>com.sun.codemodel.*</package-name>
		<package-name>com.sun.istack.*</package-name>
		<package-name>com.sun.mail.*</package-name>
		<package-name>com.sun.tools.*</package-name>
		<package-name>com.sun.xml.*</package-name>
		-->
		<package-name>com.ziclix.python.*</package-name>
		<package-name>javassist.*</package-name>
		<package-name>javassist.bytecode.*</package-name>
		<package-name>javassist.bytecode.analysis.*</package-name>
		<package-name>javassist.bytecode.annotation.*</package-name>
