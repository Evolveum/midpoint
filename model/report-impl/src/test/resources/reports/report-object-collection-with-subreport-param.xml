<report oid="2b44aa2e-dd86-4842-bcf5-762c8a9a85ed">
    <name>Object Collection report with subreport parameter</name>
    <objectCollection>
        <collection>
            <filter>
                <equal>
                    <path>name</path>
                    <expression>
                        <script>
                            <code>'will'</code>
                        </script>
                    </expression>
                </equal>
            </filter>
        </collection>
        <subreport>
            <name>testRole</name>
            <type>string</type>
            <expression>
                <script>
                    <code>
                        List list = new ArrayList();
                        list.add("TestRole1230");
                        list.add("TestRole123010");
                        return list;
                    </code>
                </script>
            </expression>
        </subreport>
        <view>
            <type>UserType</type>
            <column>
                <name>nameColumn</name>
                <path>name</path>
                <display>
                    <label>Name</label>
                </display>
            </column>
            <column>
                <name>nameColumn2</name>
                <path>name</path>
                <display>
                    <label>Name2</label>
                </display>
                <previousColumn>nameColumn</previousColumn>
                <export>
                    <expression>
                        <script>
                            <code>
                                return testRole;
                            </code>
                        </script>
                    </expression>
                </export>
            </column>
        </view>
        <useOnlyReportView>true</useOnlyReportView>
    </objectCollection>
</report>
