<objectCollection oid="00000000-0000-0000-0001-000000gh1234">
    <name>Audit records empty</name>
    <type>AuditEventRecordType</type>
    <filter>
        <and>
            <equal>
                <path>eventStage</path>
                <value>execution</value>
            </equal>
            <greater>
                <path>timestamp</path>
                <value>9999-01-01T00:00:00.000+02:00</value>
            </greater>
        </and>
    </filter>
</objectCollection>

