<objectCollection oid="11b1f98e-f587-4b9f-b92b-72e251dbb244">
    <name>Account shadow R1 with view</name>
    <type>ShadowType</type>
    <filter>
        <and>
            <ref>
                <path>resourceRef</path>
                <value oid="10000000-0000-0000-0000-000000000004"/>
            </ref>
            <equal>
                <path>kind</path>
                <value>account</value>
            </equal>
        </and>
    </filter>
    <defaultView>
        <column>
            <name>nameColumnCollection</name>
            <path>name</path>
            <display>
                <label>Name (Collection)</label>
            </display>
            <previousColumn>nameColumnReport</previousColumn>
        </column>
    </defaultView>
    <getOptions>
        <option>
            <options>
                <raw>true</raw>
            </options>
        </option>
    </getOptions>
</objectCollection>
