	xmlns:syncext="http://midpoint.evolveum.com/xml/ns/public/model/extension-3"
	xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance-3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
