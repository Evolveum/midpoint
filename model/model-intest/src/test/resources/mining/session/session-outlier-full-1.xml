<roleAnalysisSession xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
        xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        oid="0fc912c8-794a-4add-ad61-a7013d6abd4a"
        version="5">
    <name>session_outlier_full</name>
    <iteration>0</iteration>
    <iterationToken/>
    <analysisOption>
        <analysisProcedureType>outlier_detection</analysisProcedureType>
        <processMode>user</processMode>
        <analysisCategory>advanced</analysisCategory>
    </analysisOption>
    <userModeOptions>
        <similarityThreshold>80.0</similarityThreshold>
        <minMembersCount>10</minMembersCount>
        <minPropertiesOverlap>2</minPropertiesOverlap>
        <clusteringAttributeSetting>
            <clusteringAttributeRule>
                <path>parentOrgRef</path>
                <similarity>100.0</similarity>
                <weight>1.0</weight>
            </clusteringAttributeRule>
        </clusteringAttributeSetting>
        <userAnalysisAttributeSetting>
            <path>c:title</path>
            <path>c:parentOrgRef</path>
            <path>c:archetypeRef</path>
            <path>c:locality</path>
        </userAnalysisAttributeSetting>
        <isIndirect>true</isIndirect>
        <detailedAnalysis>true</detailedAnalysis>
    </userModeOptions>
    <defaultDetectionOption>
        <sensitivity>70.0</sensitivity>
        <minRolesOccupancy>2</minRolesOccupancy>
        <minUserOccupancy>2</minUserOccupancy>
        <frequencyThreshold>50.0</frequencyThreshold>
        <standardDeviation>
            <min>2.0</min>
            <max>2.0</max>
        </standardDeviation>
    </defaultDetectionOption>
</roleAnalysisSession>
