<mark xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
    oid="00000000-0000-0000-0000-000000000805">
    <name>Ignored</name>
    <documentation> <!-- TODO terminology -->
        Marks shadows that should be ignored by midPoint. They are managed externally. MidPoint should not modify them.
    </documentation>
    <assignment id="1">
        <identifier>archetype</identifier>
        <targetRef oid="00000000-0000-0000-0000-000000000701" type="ArchetypeType"/>
    </assignment>

    <!-- TODO reconsider operation policy -->
    <objectOperationPolicy>
        <!--  Model Level  -->
        <synchronize>
            <outbound>
                <enabled>false</enabled>
                <severity>error</severity>
            </outbound>
            <toleranceOverride>true</toleranceOverride>
        </synchronize>
        <!--  Provisioning level -->
        <add>
            <enabled>false</enabled>
            <severity>error</severity>
        </add>
        <modify>
            <enabled>false</enabled>
            <severity>error</severity>
        </modify>
        <delete>
            <enabled>false</enabled>
            <severity>error</severity>
        </delete>
    </objectOperationPolicy>
</mark>
