<objectTemplate
        xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
        oid="de691036-ecb0-4fce-9823-2393bcff3cbd">
    <name>scripted</name>
    <mapping>
        <strength>strong</strength>
        <source>
            <path>parentOrgRef</path>
        </source>
        <expression>
            <script>
                <code>operation + ':' + parentOrgRef?.class?.simpleName</code>
            </script>
        </expression>
        <target>
            <path>organizationalUnit</path>
        </target>
        <evaluationPhase>afterAssignments</evaluationPhase>
    </mapping>
</objectTemplate>
