<s:executeScript
        xmlns:s="http://midpoint.evolveum.com/xml/ns/public/model/scripting-3"
        xmlns:q="http://prism.evolveum.com/xml/ns/public/query-3">
    <s:search>
        <s:type>TaskType</s:type>
        <s:query>
            <q:filter>
                <q:equal>
                    <q:path>name</q:path>
                    <q:value>Task to resume</q:value>
                </q:equal>
            </q:filter>
        </s:query>
        <s:action>
            <s:type>resume</s:type>
        </s:action>
    </s:search>
</s:executeScript>
