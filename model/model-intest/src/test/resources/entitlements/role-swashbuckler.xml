<role oid="10000000-0000-0000-0000-000000001601"
        xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
        xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance-3">
    <name>Swashbuckler</name>
    <description>Scurvy Swashbuckler</description>
    <inducement>
        <construction>
            <resourceRef oid="10000000-0000-0000-0000-000000000004" type="c:ResourceType"/>
            <kind>account</kind>
            <attribute>
                <ref>ri:title</ref>
                <outbound>
                    <strength>strong</strength>
                    <expression>
                        <value>Swashbuckler</value>
                    </expression>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:drink</ref>
                <outbound>
                    <strength>strong</strength>
                    <expression>
                        <value>grog</value>
                    </expression>
                </outbound>
            </attribute>
            <association>
                <ref>ri:group</ref>
                <outbound>
                    <strength>strong</strength>
                    <expression>
                        <value xsi:type="ShadowAssociationValueType">
                            <objects>
                                <ri:group oid="20000000-0000-0000-3333-000000000001"/>
                            </objects>
                        </value>
                    </expression>
                </outbound>
            </association>
        </construction>
    </inducement>
</role>
