<role oid="181a58ae-90dd-11e8-a371-77713d9f7a57"
        xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
        xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
        xmlns:t="http://prism.evolveum.com/xml/ns/public/types-3"
        xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance-3"
        xmlns:piracy="http://midpoint.evolveum.com/xml/ns/samples/piracy">
    <name>Blue Swashbuckler</name>
    <description>Scurvy Blue Swashbuckler</description>
    <inducement>
        <construction>
            <resourceRef oid="10000000-0000-0000-0000-000000000204" type="c:ResourceType"/> <!-- Dummy Blue -->
            <kind>account</kind>
            <attribute>
                <ref>ri:title</ref>
                <outbound>
                    <strength>strong</strength>
                    <expression>
                        <value>Swashbuckler</value>
                    </expression>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:drink</ref>
                <outbound>
                    <strength>strong</strength>
                    <expression>
                        <value>grog</value>
                    </expression>
                </outbound>
            </attribute>
            <association>
                <ref>ri:group</ref>
                <outbound>
                    <strength>strong</strength>
                    <expression>
                        <value>
                            <shadowRef oid="20000000-0000-0000-3333-020400000001" type="ShadowType"/>
                        </value>
                    </expression>
                </outbound>
            </association>
        </construction>
    </inducement>
</role>
