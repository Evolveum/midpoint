      oid="00000001-d34d-b33f-f00d-000000000012">

    <!-- Auto approval with no approvers defined - allMustApprove -->

    <name>Role12</name>
    <approvalSchema>
        <stage>
            <approverExpression>
                <script>
                    <code>
                        null
                    </code>
                </script>
            </approverExpression>
            <evaluationStrategy>allMustApprove</evaluationStrategy>
            <outcomeIfNoApprovers>approve</outcomeIfNoApprovers>
        </stage>
    </approvalSchema>

</role>
