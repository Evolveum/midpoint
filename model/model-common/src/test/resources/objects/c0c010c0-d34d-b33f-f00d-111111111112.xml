<c:user oid="c0c010c0-d34d-b33f-f00d-111111111112" 
   xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
   xmlns:xsd='http://www.w3.org/2001/XMLSchema'
   xmlns:c='http://midpoint.evolveum.com/xml/ns/public/common/common-3'
   xmlns:t="http://prism.evolveum.com/xml/ns/public/types-3"
   xmlns:piracy='http://midpoint.evolveum.com/xml/ns/samples/piracy'>
   <!-- This is parsed by JAXB in the tests. Therefore we need to provide this in full -->
    <c:name>
    	<t:orig>barbossa</t:orig>
    	<t:norm>barbossa</t:norm>
    </c:name>
    <!-- Description is a string as opposed to name which is PolyString. We use description instead of
         name in tests where we need string. -->
    <c:description>barbossa</c:description>
    <c:extension>
        <piracy:ship xsi:type="xsd:string">Black Pearl</piracy:ship>
    </c:extension>
    <c:fullName>Hector Barbossa</c:fullName>
    <c:givenName>Hector</c:givenName>
    <c:familyName>Basbossa</c:familyName>
    <c:honorificPrefix>Cpt.</c:honorificPrefix>    
</c:user>
