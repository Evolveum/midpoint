 <!-- Testing resource. File reusable for many tests. -->

<c:object xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-2"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="c:ResourceType" oid="aae7be60-df56-11df-8608-0002a5d5c51b">
    <c:name>Localhost DatabaseTable</c:name>
    <c:namespace>http://midpoint.evolveum.com/xml/ns/public/resource/instances/aae7be60-df56-11df-8608-0002a5d5c51b</c:namespace>
    <c:configuration>
        <ns2:ConnectorConfiguration xmlns:ns2="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/configuration-1.xsd">
            <ns2:ConnectorRef bundleName="org.identityconnectors.databasetable" bundleVersion="1.1.4958" connectorName="org.identityconnectors.databasetable.DatabaseTableConnector">
                <ns2:ConnectorHostRef>/configuration/connectorHost[@oid='1234']/configuration</ns2:ConnectorHostRef>
            </ns2:ConnectorRef>
            <ns2:BundleProperties>
                <iccldap:jdbcDriver xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.databasetable/org.identityconnectors.databasetable.DatabaseTableConnector/1.1.4958">org.apache.derby.jdbc.EmbeddedDriver</iccldap:jdbcDriver>
                <iccldap:jdbcUrlTemplate xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.databasetable/org.identityconnectors.databasetable.DatabaseTableConnector/1.1.4958">jdbc:derby:target/testdb</iccldap:jdbcUrlTemplate>
                <iccldap:table xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.databasetable/org.identityconnectors.databasetable.DatabaseTableConnector/1.1.4958">account</iccldap:table>
                <iccldap:keyColumn xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.databasetable/org.identityconnectors.databasetable.DatabaseTableConnector/1.1.4958">id</iccldap:keyColumn>
                <iccldap:passwordColumn xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.databasetable/org.identityconnectors.databasetable.DatabaseTableConnector/1.1.4958">password</iccldap:passwordColumn>
                <iccldap:changeLogColumn xmlns:iccldap="http://midpoint.evolveum.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.databasetable/org.identityconnectors.databasetable.DatabaseTableConnector/1.1.4958">changelog</iccldap:changeLogColumn>
            </ns2:BundleProperties>
            <ns2:PoolConfigOption minEvictTimeMillis="5000" minIdle="5" maxIdle="30" maxObjects="120" maxWait="5000"/>
            <ns2:OperationTimeouts>
                <ns2:OperationTimeout name="create" timeout="50000"/>
                <ns2:OperationTimeout name="update" timeout="50000"/>
                <ns2:OperationTimeout name="delete" timeout="50000"/>
                <ns2:OperationTimeout name="test" timeout="50000"/>
                <ns2:OperationTimeout name="scriptOnConnector" timeout="50000"/>
                <ns2:OperationTimeout name="scriptOnResource" timeout="50000"/>
                <ns2:OperationTimeout name="get" timeout="50000"/>
                <ns2:OperationTimeout name="authenticate" timeout="50000"/>
                <ns2:OperationTimeout name="search" timeout="50000"/>
                <ns2:OperationTimeout name="validate" timeout="50000"/>
                <ns2:OperationTimeout name="sync" timeout="50000"/>
                <ns2:OperationTimeout name="schema" timeout="50000"/>
            </ns2:OperationTimeouts>
        </ns2:ConnectorConfiguration>
    </c:configuration>
    <c:schema>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
targetNamespace="http://midpoint.evolveum.com/xml/ns/public/resource/instances/aae7be60-df56-11df-8608-0002a5d5c51b"
 xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-2"
xmlns:xns0="http://midpoint.evolveum.com/xml/ns/public/resource/instances/aae7be60-df56-11df-8608-0002a5d5c51b"
xmlns:xns1="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/resource-schema-2.xsd">
            <xsd:import namespace="http://midpoint.evolveum.com/xml/ns/public/resource/idconnector/resource-schema-2.xsd"/>
            <xsd:import namespace="http://midpoint.evolveum.com/xml/ns/public/common/common-2"/>
            
            <xsd:complexType name="Account">
                <xsd:annotation>
                    <xsd:appinfo>
                        <r:identifier  ref="xns1:__UID__"/>
                        <r:secondaryIdentifier  ref="xns1:__NAME__"/>
                        <r:compositeIdentifier  ref="xns0:sn"/>
                        <r:displayName  ref="xns0:description"/>
                        <r:descriptionAttribute  ref="xns0:description"/>
                        <r:nativeObjectClass >__ACCOUNT__</r:nativeObjectClass>
                        <r:accountType default="true" />
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:complexContent>
                    <xsd:extension base="r:ResourceObjectClass" >
                        <xsd:sequence>
                            <xsd:element ref="xns1:__NAME__">
                                <xsd:annotation>
                                    <xsd:appinfo>
                                        <r:nativeAttributeName >__NAME__</r:nativeAttributeName>
                                        <r:attributeDisplayName>Some custom name</r:attributeDisplayName>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="description" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:appinfo>
                                        <r:help >DESCRIPTION_NAME_HELP_KEY</r:help>
                                        <r:nativeAttributeName >description</r:nativeAttributeName>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element ref="xns1:__PASSWORD__">
                                <xsd:annotation>
                                    <xsd:appinfo>
                                        <r:attributeFlag >PASSWORD</r:attributeFlag>
                                        <r:classifiedAttribute >
                                            <r:encryption>HASH</r:encryption>
                                            <r:classificationLevel>password</r:classificationLevel>
                                        </r:classifiedAttribute>
                                        <r:nativeAttributeName >__PASSWORD__</r:nativeAttributeName>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="sn" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:appinfo>
                                        <r:nativeAttributeName >sn</r:nativeAttributeName>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="cn" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:appinfo>
                                        <r:nativeAttributeName >cn</r:nativeAttributeName>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="groups" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:appinfo>
                                        <r:nativeAttributeName >groups</r:nativeAttributeName>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="custom" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:appinfo>
                                        <r:attributeFlag >IGNORE_ATTRIBUTE</r:attributeFlag>
                                        <r:help >This attribute part of the SchemaHandling</r:help>
                                        <r:nativeAttributeName >custom</r:nativeAttributeName>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element ref="xns1:__UID__">
                                <xsd:annotation>
                                    <xsd:appinfo>
                                        <r:attributeFlag >NOT_UPDATEABLE</r:attributeFlag>
                                        <r:nativeAttributeName >__UID__</r:nativeAttributeName>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="givenName" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:appinfo>
                                        <r:nativeAttributeName >givenName</r:nativeAttributeName>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element minOccurs="0" name="fullName" type="xsd:string">
                                <xsd:annotation>
                                    <xsd:appinfo>
                                        <r:help >DISPLAY_NAME_HELP_KEY</r:help>
                                        <r:nativeAttributeName >fullName</r:nativeAttributeName>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
        </xsd:schema>
    </c:schema>
    <c:schemaHandling>
        <!-- No need to schemaHandling in this case. Everything is in schema (annotations) -->
    </c:schemaHandling>
	
		<c:scripts>
            <c:script>
                <c:operation>add</c:operation>
                <c:order>after</c:order>
                <c:host>connector</c:host>
                <c:language>sh</c:language>
                <c:argument>
                    <c:valueExpression>
                        declare namespace i="http://midpoint.evolveum.com/xml/ns/public/common/common-2";
                        declare namespace unix="http://midpoint.evolveum.com/xml/ns/public/experiments/unix-1.xsd";
                        $c:user/c:extension/unix:homedir
                    </c:valueExpression>
                    <c:name>HOMEDIR</c:name>
                </c:argument>
                <c:code>mkdir $HOMEDIR</c:code>
            </c:script>
    </c:scripts>

</c:object>
