<pipeline list="true">            <!-- this is necessary, because 'sequence' is ambiguous (c:sequence, s:sequence) so we cannot afford to guess here -->
    <sequence>
        <search>
            <type>RoleType</type>
            <searchFilter>
                <equal>
                    <path>name</path>
                    <value>AAAAA</value>
                </equal>
            </searchFilter>
        </search>
        <action>
            <type>log</type>
        </action>
    </sequence>
    <sequence>
        <action>
            <type>delete</type>
        </action>
        <action>
            <type>assign</type>
            <parameter>
                <name>role</name>
                <value>rome555c-7797-11e2-94a6-001e8c717e5b</value>
            </parameter>
        </action>
        <search>
            <type>UserType</type>
        </search>
    </sequence>
</pipeline>
