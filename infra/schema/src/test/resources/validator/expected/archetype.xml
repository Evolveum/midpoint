<archetype xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
        oid="3f2b893c-8732-4549-9f79-73ad2472cc4f">

    <name>archetype</name>

    <archetypePolicy>
        <itemConstraint id="1">
            <path>name</path>
            <visibility>visible</visibility>
        </itemConstraint>
        <itemConstraint id="2">
            <path>givenName</path>
            <visibility>hidden</visibility>
            <oidBound>true</oidBound>
        </itemConstraint>
        <itemConstraint id="3">
            <path>familyName</path>
            <visibility>visible</visibility>
        </itemConstraint>
    </archetypePolicy>
</archetype>
