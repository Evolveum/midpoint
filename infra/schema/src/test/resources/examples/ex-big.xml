    <m:role oid="c0c010c0-d34d-b33f-f00d-444111111111">
        <c:name>Captain</c:name>
        <m:impliedAccount>
            <m:resourceRef oid="c0c010c0-d34d-b33f-f00d-333111111111" type="ResourceType"/>
            <m:type>default</m:type>
            <m:condition>
                <!-- Captain without a ship is not a captain! -->
                boolean($user/c:extension/piracy:ship)
            </m:condition>
            <m:attribute exclusive="true">
                <m:value>
                    <dj:l>bridge</dj:l>
                </m:value>
            </m:attribute>
            <m:attribute clear="true">
                <m:valueExpression ref="dj:description">
                        concat('Captain ', $user/i:givenName)
                </m:valueExpression>
            </m:attribute>
            <m:entitlement>
                <m:value>
                    <dj:dn>cn=captains,ou=groups,dc=example,dc=com</dj:dn>
                </m:value>
                <m:type>groupOfNames</m:type>
            </m:entitlement>
            <m:entitlement authoritative="true">
                <m:valueExpression ref="dj:dn">
                        concat('cn=', $user/c:extension/piracy:ship, ',ou=groups,dc=example,dc=com')
                </m:valueExpression>
                <m:condition>
                    boolean($user/c:extension/piracy:ship)
                </m:condition>
                <m:type>groupOfNames</m:type>
            </m:entitlement>
        </m:impliedAccount>
    </m:role>

