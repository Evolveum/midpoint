<query xmlns="http://prism.evolveum.com/xml/ns/public/query-3" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3">
    <filter>
        <or>
            <and>
                <greater>
                    <path>c:costCenter</path>
                    <value>100000</value>
                </greater>
                <less>
                    <path>c:costCenter</path>
                    <value>999999</value>
                </less>
            </and>
            <and>
                <greaterOrEqual>
                    <path>c:costCenter</path>
                    <value>X100</value>
                </greaterOrEqual>
                <lessOrEqual>
                    <path>c:costCenter</path>
                    <value>X999</value>
                </lessOrEqual>
            </and>
        </or>
    </filter>
</query>
