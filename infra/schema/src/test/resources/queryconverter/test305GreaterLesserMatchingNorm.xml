<query xmlns="http://prism.evolveum.com/xml/ns/public/query-3" xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3">
    <filter>
        <and>
            <greaterOrEqual>
                <matching>polyStringNorm</matching>
                <path>c:name</path>
                <value>00</value>
            </greaterOrEqual>
            <less>
                <matching>polyStringNorm</matching>
                <path>c:name</path>
                <value>0a</value>
            </less>
        </and>
    </filter>
</query>
