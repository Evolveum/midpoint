<query xmlns:q="http://prism.evolveum.com/xml/ns/public/query-3"><filter>
    <q:type>
        <q:type>RoleType</q:type>
        <q:or>
            <q:equal>
                <q:path>name</q:path>
                <q:value>My Example 1</q:value>
            </q:equal>
            <q:substring>
                <q:path>name</q:path>
                <q:value>My Example 2</q:value>
                <q:anchorStart>true</q:anchorStart>
            </q:substring>
        </q:or>
    </q:type>
</filter>
</query>
