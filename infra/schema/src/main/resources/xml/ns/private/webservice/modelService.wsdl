<definitions name="modeService"
    targetNamespace="http://midpoint.evolveum.com/xml/ns/public/model/model-3"
    xmlns:tns="http://midpoint.evolveum.com/xml/ns/public/model/model-3"
    xmlns="http://schemas.xmlsoap.org/wsdl/"
