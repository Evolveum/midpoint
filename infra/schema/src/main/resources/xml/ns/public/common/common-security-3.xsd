        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:AbstractTokenOidcResourceServerType">
                <xsd:sequence>
                    <xsd:element name="userInfoUri" type="xsd:string" minOccurs="0" maxOccurs="1">
