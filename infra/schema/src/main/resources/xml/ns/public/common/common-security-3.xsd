                            <xsd:documentation>
                                Realm which Midpoint use for WWW-Authentication header
                            </xsd:documentation>
                            <xsd:appinfo>
                                <a:since>4.2</a:since>
                            </xsd:appinfo>
                        </xsd:annotation>
