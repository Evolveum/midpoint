		<!--<xsd:restriction base="xsd:string">-->
			<!--<xsd:enumeration value="asIs">-->
				<!--<xsd:annotation>-->
					<!--<xsd:documentation>-->
						<!--Triggers for situation constraints should be stored "as is".-->
					<!--</xsd:documentation>-->
					<!--<xsd:appinfo>-->
						<!--<jaxb:typesafeEnumMember name="AS_IS"/>-->
					<!--</xsd:appinfo>-->
