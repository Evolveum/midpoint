                    </xsd:documentation>
                    <xsd:appinfo>
                        <a:displayName>FunctionLibraryProfileType.description</a:displayName>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element ref="tns:documentation" minOccurs="0"/>
            <xsd:element name="decision" type="tns:AuthorizationDecisionType">
