                           </xsd:documentation>
                        <xsd:appinfo>
                            <a:displayName>GuiExportSettingsType.sizeLimit</a:displayName>
                        </xsd:appinfo>
                       </xsd:annotation>
                   </xsd:element>
                   <!-- TODO: later: default data language, sync/async, whether to show detail dialog or just run the export, etc. -->
               </xsd:sequence>
