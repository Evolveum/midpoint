                    </xsd:documentation>
                    <xsd:appinfo>
                        <a:displayName>FunctionLibraryProfileType.ref</a:displayName>
                        <a:objectReferenceTargetType>tns:FunctionLibraryType</a:objectReferenceTargetType>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="description" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
