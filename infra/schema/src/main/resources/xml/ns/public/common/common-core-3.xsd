                    </xsd:documentation>
                    <xsd:appinfo>
                        <a:displayName>ExpressionPermissionMethodProfileType.description</a:displayName>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element ref="tns:documentation" minOccurs="0"/>
            <xsd:element name="decision" type="tns:AuthorizationDecisionType">
