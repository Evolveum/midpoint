            <xsd:documentation>
                An aspect of shadow integrity to be diagnosed and/or fixed.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="intents">
                <xsd:annotation>
                    <xsd:documentation>
