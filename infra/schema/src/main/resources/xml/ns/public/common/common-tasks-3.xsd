                        </xsd:documentation>
                        <xsd:appinfo>
                            <a:since>4.8</a:since>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="certificationRemediation" type="tns:CertificationRemediationWorkDefinitionType"
