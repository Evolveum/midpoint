    <xsd:complexType name="AccessCertificationConfigurationType">
        <xsd:annotation>
            <xsd:documentation>
                Configuration for access certification.
            </xsd:documentation>
            <xsd:appinfo>
                <a:container/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="availableResponse" minOccurs="0" maxOccurs="unbounded" type="tns:AccessCertificationResponseType">
                <xsd:annotation>
                    <xsd:documentation>
                        What response types are available to reviewers? Empty list means all responses.
                        TODO should we configure also names of the responses? (probably no)

                        DEPRECATED configure actions in the cert. items collection view instead.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <a:displayName>AccessCertificationConfigurationType.availableResponse</a:displayName>
                        <a:deprecatedSince>4.9</a:deprecatedSince>
                        <a:deprecated/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="reviewerCommentsFormatting" minOccurs="0" type="tns:PerformerCommentsFormattingType">
                <xsd:annotation>
                    <xsd:documentation>
                        Instructions how to format reviewers comments before storing them into metadata.
                        EXPERIMENTAL
                    </xsd:documentation>
                    <xsd:appinfo>
                        <a:experimental>true</a:experimental>
                        <a:since>3.7.1</a:since>
                        <a:displayName>AccessCertificationConfigurationType.reviewerCommentsFormatting</a:displayName>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="allowCertificationItemsMenus" minOccurs="0" type="xsd:boolean" default="false">
                <xsd:annotation>
                    <xsd:documentation>
                        Defines if the certification items menu items should be added to the left menu so that
                        "Certification items" and "My certification items" pages can be reached directly from the main menu.
                        If not defined (or set to false), the certification items pages can be reached only from the active
                        campaigns panel.
                        This configuration option was added to support old behavior where the certification items pages had their
                        own menu items. The default value is false. By default, the user sees "Active campaigns" and
                        "My active campaigns" menu items going through which certification items page can be reached.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="multiselect" type="tns:MultiselectOptionType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Configures the possibility of selecting multiple objects in the table or view
                        (in case this table or view provides item selection functionality).
                    </xsd:documentation>
                    <xsd:appinfo>
                        <a:displayName>GuiObjectListPanelConfigurationType.multiselect</a:displayName>
                        <a:since>4.9</a:since>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="defaultView" type="tns:GuiObjectListViewType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Specifies a default view for a presentation of the certification items collection.
                        </p>
                    </xsd:documentation>
                    <xsd:appinfo>
                        <a:since>4.9</a:since>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:long"/>
    </xsd:complexType>

    <xsd:simpleType name="MultiselectOptionType">
        <xsd:annotation>
            <xsd:documentation>
                Defines the possibility to select multiple items.
            </xsd:documentation>
            <xsd:appinfo>
                <a:since>4.9</a:since>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="selectAll">
                <xsd:annotation>
                    <xsd:documentation>
                        Gives the possibility to select all items at once.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="SELECT_ALL"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="selectIndividualItems">
                <xsd:annotation>
                    <xsd:documentation>
                        Gives the possibility to select individual items without possibility to select all at once.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="SELECT_INDIVIDUAL_ITEMS"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="noSelect">
                <xsd:annotation>
                    <xsd:documentation>
                        No possibility to select any items.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="NO_SELECT"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

</xsd:schema>
