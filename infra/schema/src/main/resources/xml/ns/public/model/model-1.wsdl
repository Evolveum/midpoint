                      ObjectNotFoundFaultType: object with specified OID does not exists
            </documentation>
            <input name="findShadowOwnerInput" message="tns:findShadowOwner"/>
            <output name="findShadowOwnerOutput" message="tns:findShadowOwnerResponse"/>
            <fault name="fault" message="f:FaultMessage"/>
        </operation>

