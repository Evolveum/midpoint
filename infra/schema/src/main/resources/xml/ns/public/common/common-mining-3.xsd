    <xsd:element name="identifiedCharacteristics" type="tns:RoleAnalysisIdentifiedCharacteristicsType"/>

    <!--TODO: better naming-->
    <xsd:complexType name="RoleAnalysisIdentifiedCharacteristicsItemsType">
        <xsd:annotation>
            <xsd:documentation>
                Contains specific characteristics of the object.
            </xsd:documentation>
            <xsd:appinfo>
                <a:container>true</a:container>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>

            <xsd:element name="unPopularCount" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Number of unpopular objects.
                        </p>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="abovePopularCount" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Number of above popular objects.
                        </p>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="noiseCount" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Number of noise objects.
                        </p>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="noiseExclusiveCount" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Number of noise exclusive objects.
                        </p>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="anomalyCount" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Number of anomaly objects.
                        </p>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="overallAnomalyCount" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Number of overall anomaly objects.
                        </p>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="outlierCount" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Number of outlier objects.
                        </p>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="excludedCount" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Number of excluded objects.
                        </p>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="insufficientCount" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Number of insufficient per similarity.
                        </p>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="noiseExclusiveUnpopular" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Number of noise exclusive unpopular objects (noise exclusive and unpopular).
                        </p>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="excludedMissingBase" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        <p>
                            Number of excluded missing base objects.
                        </p>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="item" type="tns:RoleAnalysisIdentifiedCharacteristicsItemType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>
                        Roles with identified characteristics.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:long"/>
    </xsd:complexType>
    <xsd:element name="items" type="tns:RoleAnalysisIdentifiedCharacteristicsItemsType"/>

    <!--TODO: better naming-->
    <xsd:complexType name="RoleAnalysisIdentifiedCharacteristicsItemType">
        <xsd:annotation>
            <xsd:documentation>
                Contains specific characteristics of the object.
            </xsd:documentation>
            <xsd:appinfo>
                <a:container>true</a:container>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>

            <xsd:element name="objectRef" type="tns:ObjectReferenceType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Reference to the object that has been identified.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="category" type="tns:RoleAnalysisObjectCategorizationType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>
                        Category classification.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:long"/>
    </xsd:complexType>
    <xsd:element name="item" type="tns:RoleAnalysisIdentifiedCharacteristicsItemType"/>

    <!--TODO: better naming-->
    <xsd:simpleType name="RoleAnalysisObjectCategorizationType">
        <xsd:annotation>
            <xsd:documentation>
                Category classification.
            </xsd:documentation>
            <xsd:appinfo>
                <a:experimental>true</a:experimental>
                <a:since>4.8</a:since>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="un_popular">
                <xsd:annotation>
                    <xsd:documentation>
                        Unpopular roles or users.
                        Objects that are not frequently used or accessed.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="UN_POPULAR"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="above_popular">
                <xsd:annotation>
                    <xsd:documentation>
                        Above popular roles or users.
                        Objects that are frequently used or accessed.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="ABOVE_POPULAR"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="noise_exclusive">
                <xsd:annotation>
                    <xsd:documentation>
                        Noise exclusive roles or users.
                        Objects that appears only in noise clusters.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="NOISE_EXCLUSIVE"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="noise">
                <xsd:annotation>
                    <xsd:documentation>
                        Objects that appears in noise clusters.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="NOISE"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="overall_anomaly">
                <xsd:annotation>
                    <xsd:documentation>
                        Anomaly roles or users.
                        Currently, user-mode outlier detection, this category is used only if role is
                        marked as anomaly in every analysis user object.
                        (In the future, it will be used for more complex role mode analysis)
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="OVERALL_ANOMALY"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="anomaly">
                <xsd:annotation>
                    <xsd:documentation>
                        Anomaly roles or users.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="ANOMALY"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="outlier">
                <xsd:annotation>
                    <xsd:documentation>
                        Outlier roles or users.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="OUTLIER"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="insufficient">
                <xsd:annotation>
                    <xsd:documentation>
                        Insufficient peer similarity (noise exclusive = noise - unpopular).
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="INSUFFICIENT"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="noise_exclusive_unpopular">
                <xsd:annotation>
                    <xsd:documentation>
                        (noise_exclusive_unpopular = noise exclusive and unpopular).
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="NOISE_EXCLUSIVE_UNPOPULAR"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="excluded">
                <xsd:annotation>
                    <xsd:documentation>
                        Outlier roles or users.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="EXCLUDED"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="excluded_missing_base">
                <xsd:annotation>
                    <xsd:documentation>
                        If object is excluded due to missing member/assignment.
                        For example user "Jack" has role A,B,C and these roles is unpopular with means that is excluded from analysis.
                        That means that user "Jack" is excluded from analysis due to missing roles.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="EXCLUDED_MISSING_BASE"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>


    <xsd:complexType name="OutlierDetectionExplanationType">
        <xsd:annotation>
            <xsd:documentation>
                Contains explanation of the outlier or anomaly object.
            </xsd:documentation>
            <xsd:appinfo>
                <a:container>true</a:container>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>

            <xsd:element name="message" type="tns:LocalizableMessageType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Explanation message.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="category" type="tns:OutlierDetectionExplanationCategoryType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>
                        Category classification.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:long"/>
    </xsd:complexType>
    <xsd:element name="outlierDetectionExplanationType" type="tns:OutlierDetectionExplanationType"/>

    <xsd:simpleType name="OutlierDetectionExplanationCategoryType">
        <xsd:annotation>
            <xsd:documentation>
                Category classification.
            </xsd:documentation>
            <xsd:appinfo>
                <a:experimental>true</a:experimental>
                <a:since>4.8</a:since>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="irregular_attributes">
                <xsd:annotation>
                    <xsd:documentation>
                        Explanations associated with role attributes analysis.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="IRREGULAR_ATTRIBUTES"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="unusual_access">
                <xsd:annotation>
                    <xsd:documentation>
                        Explanation associated with peer group analysis.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="UNUSUAL_ACCESS"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="FrequencyType">
        <xsd:annotation>
            <xsd:documentation>
                Represents frequency-related information.
            </xsd:documentation>
            <xsd:appinfo>
                <a:container>true</a:container>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>

            <xsd:element name="percentageRatio" type="xsd:double" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Providing information about the percentage-based
                        frequency.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="valueRatio" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Providing information about the number-based
                        frequency.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="entiretyCount" type="xsd:int" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Providing information about entire count of objects.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:long"/>
    </xsd:complexType>
    <xsd:element name="frequencyType" type="tns:FrequencyType"/>

</xsd:schema>


