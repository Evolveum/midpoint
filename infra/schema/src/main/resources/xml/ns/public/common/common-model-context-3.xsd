                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="traceDataFlowVisualizationInstruction" type="tns:TraceDataFlowVisualizationInstructionType"/>
</xsd:schema>
