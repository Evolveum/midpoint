        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:ActionExpressionType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="testResource" type="tns:TestResourceActionExpressionType" substitutionGroup="tns:scriptingExpression">
