                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="resolveReference" type="tns:ResolveReferenceActionExpressionType" substitutionGroup="tns:scriptingExpression">
        <xsd:annotation>
            <xsd:appinfo>
                <a:heterogeneousListItem/>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>

    <xsd:complexType name="ResumeTaskActionExpressionType">
        <xsd:annotation>
            <xsd:documentation>
                Statically-typed "resume" action.
            </xsd:documentation>
            <xsd:appinfo>
                <a:since>4.2</a:since>
                <a:experimental>true</a:experimental>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:ActionExpressionType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="resumeTask" type="tns:ResumeTaskActionExpressionType" substitutionGroup="tns:scriptingExpression">
        <xsd:annotation>
            <xsd:appinfo>
                <a:heterogeneousListItem/>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>

    <xsd:complexType name="TestResourceActionExpressionType">
        <xsd:annotation>
            <xsd:documentation>
