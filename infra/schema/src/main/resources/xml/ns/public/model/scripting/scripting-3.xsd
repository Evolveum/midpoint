            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>

    <xsd:complexType name="RecomputeActionExpressionType">
        <xsd:annotation>
            <xsd:documentation>
                Statically-typed "recompute" action.
            </xsd:documentation>
            <xsd:appinfo>
                <a:since>4.2</a:since>
                <a:experimental>true</a:experimental>
