    <xsd:complexType name="GuiParameterType">
        <xsd:annotation>
            <xsd:documentation>
                Parameter which can be used within GUI.
            </xsd:documentation>
            <xsd:appinfo>
                <a:container>true</a:container>
                <a:since>4.9</a:since>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="c:ParameterType">
                <xsd:sequence>
                    <xsd:element name="display" type="tns:DisplayType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>
                                Defines the characteristics of the parameter to be displayed in the GUI.
                                E.g. label, help text, etc.
                            </xsd:documentation>
                            <xsd:appinfo>
                                <a:displayName>GuiParameterType.display</a:displayName>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="mandatory" type="xsd:boolean" minOccurs="0" default="false">
                        <xsd:annotation>
                            <xsd:documentation>
                                Defines if the parameter is mandatory.
                            </xsd:documentation>
                            <xsd:appinfo>
                                <a:displayName>GuiParameterType.mandatory</a:displayName>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="path" type="t:ItemPathType" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>
                                Defines path to the attribute.
                            </xsd:documentation>
                            <xsd:appinfo>
                                <a:displayName>GuiParameterType.path</a:displayName>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="manual" type="xsd:boolean" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>
                                Defines if the parameter is requested to be set by user
                            </xsd:documentation>
                            <xsd:appinfo>
                                <a:displayName>GuiParameterType.manual</a:displayName>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

