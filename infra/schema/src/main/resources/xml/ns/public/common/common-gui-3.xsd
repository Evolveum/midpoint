    <xsd:complexType name="CheckoutValidityConfigurationType">
        <xsd:annotation>
            <xsd:documentation>
                Configuration of validity (assignment validity) for requested roles in last step (checkout) of request access UI wizard.
            </xsd:documentation>
            <xsd:appinfo>
                <a:container>true</a:container>
                <a:since>4.6</a:since>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
