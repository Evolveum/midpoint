        </xsd:annotation>
        <xsd:sequence>
        	<xsd:any minOccurs="0" maxOccurs="unbounded" processContents="lax"/>
        </xsd:sequence>
        <xsd:anyAttribute processContents="lax"/>
    </xsd:complexType>

    <xsd:complexType name="XmlAsStringType" mixed="true">
